<%= form_tag('/sounds/' + params[:id], method: :put) do %>
  <%= label_tag('sound[title]', 'Title:') %>
  <%= text_field(:sound, :title) %>
  <br>
  <%= label_tag('sound[soundcloud_url]', 'Soundcloud URL:') %>
  <%= text_field(:sound, :soundcloud_url) %>
  <br>
  <%= submit_tag('update sound') %>
<% end %> 
